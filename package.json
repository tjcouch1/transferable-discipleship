{
  "name": "transferable-discipleship",
  "version": "2.2.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npm run web",
    "start:public": "npm run web -- --port 20589",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start --web --go",
    "test": "jest",
    "expo:upgrade": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo install --fix",
    "postinstall": "npx patch-package",
    "licenses": "license-checker --json --out assets/data/licenses/licenses-raw.json --customPath lib/license-checker.json && ts-node lib/transform-licenses-content.ts assets/data/licenses/licenses-raw.json assets/data/licenses/licenses.json assets/fonts",
    "build:production-web": "expo export --platform web",
    "preview-build:web": "serve dist --single",
    "build:preview-android": "eas build -p android --profile preview",
    "build:preview-ios": "eas build -p ios --profile preview",
    "build:preview-all": "eas build -p all --profile preview",
    "build:production-android": "eas build -p android --profile production",
    "build:production-ios": "eas build -p ios --profile production",
    "build:production-all": "eas build -p all --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "better-react-native-image-viewing": "^0.2.7",
    "expo": "^52.0.0",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.4.1",
    "cross-env": "^7.0.3",
    "jest": "^29.4.1",
    "license-checker": "^25.0.1",
    "patch-package": "^8.0.0",
    "react-test-renderer": "^18.1.0",
    "serve": "^14.2.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.3"
  },
  "private": true,
  "author": "TJ Couch tjcouch1@gmail.com",
  "copyright": "Copyright 2023 Wesley Skinner and TJ Couch",
  "repository": {
    "type": "git",
    "url": "https://github.com/tjcouch1/transferable-discipleship"
  },
  "homepage": "https://tjcouch1.github.io/transferable-discipleship/app",
  "license": "GPL-3.0-only"
}
