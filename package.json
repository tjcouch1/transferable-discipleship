{
  "name": "transferable-discipleship",
  "version": "2.2.1-update.2",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npm run web",
    "start:public": "npm run web -- --port 20589",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start --web --go",
    "test": "jest",
    "expo:upgrade": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo install --fix",
    "postinstall": "npx patch-package",
    "licenses": "license-checker --json --out assets/data/licenses/licenses-raw.json --customPath lib/license-checker.json && node --no-warnings=ExperimentalWarning --loader ts-node/esm lib/transform-licenses-content.mts assets/data/licenses/licenses-raw.json assets/data/licenses/licenses.json assets/fonts",
    "build:preview-web": "expo export --platform web --output-dir gh-pages-staging/app",
    "preview-build:web": "serve gh-pages-staging/app",
    "build:production-web": "cross-env WEB_PROD=\"true\" npm run build:preview-web",
    "deploy:web": "gh-pages --nojekyll -d gh-pages-staging",
    "build:preview-android": "eas build -p android --profile preview",
    "build:preview-ios": "eas build -p ios --profile preview",
    "build:preview-all": "eas build -p all --profile preview",
    "build:production-android": "eas build -p android --profile production",
    "build:production-ios": "eas build -p ios --profile production",
    "build:production-all": "eas build -p all --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@sillsdev/scripture": "github:sillsdev/scripture",
    "better-react-native-image-viewing": "^0.2.7",
    "expo": "^53.0.0",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.4.1",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.3.0",
    "jest": "^29.4.1",
    "license-checker": "^25.0.1",
    "patch-package": "^8.0.0",
    "react-test-renderer": "^19.0.0",
    "serve": "^14.2.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true,
  "author": "TJ Couch tjcouch1@gmail.com",
  "copyright": "Copyright 2023 Wesley Skinner and TJ Couch",
  "repository": {
    "type": "git",
    "url": "https://github.com/tjcouch1/transferable-discipleship"
  },
  "homepage": "https://tjcouch1.github.io/transferable-discipleship/app",
  "license": "GPL-3.0-only"
}
