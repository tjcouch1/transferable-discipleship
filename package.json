{
  "name": "discipleship-app-template",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npm run web",
    "start:public": "npm run web -- --port 20589",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo start --web --go",
    "test": "jest",
    "expo:upgrade": "cross-env NODE_OPTIONS=--openssl-legacy-provider expo install --fix",
    "postinstall": "npx patch-package",
    "licenses": "license-checker --json --out assets/data/licenses/licenses-raw.json --customPath lib/license-checker.json && ts-node lib/transform-licenses-content.ts assets/data/licenses/licenses-raw.json assets/data/licenses/licenses.json assets/fonts",
    "build:preview-android": "eas build -p android --profile preview",
    "build:preview-ios": "eas build -p ios --profile preview"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "better-react-native-image-viewing": "^0.2.7",
    "expo": "~51.0.24",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.21",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.4.1",
    "cross-env": "^7.0.3",
    "jest": "^29.4.1",
    "license-checker": "^25.0.1",
    "patch-package": "^8.0.0",
    "react-test-renderer": "^18.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.3"
  },
  "private": true,
  "author": "<content_creator> <contact_email>",
  "copyright": "Copyright 2023 <content_creator>",
  "repository": {
    "type": "git",
    "url": "<repo_url>"
  },
  "license": "GPL-3.0-only"
}
